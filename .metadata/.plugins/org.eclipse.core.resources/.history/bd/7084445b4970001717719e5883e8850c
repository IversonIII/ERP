		url:'storedetail_storealertList',
		columns:[[
		    {field:'uuid',title:'商品编号',width:100},
		    {field:'storeName',title:'商品名称',width:100},
		    {field:'goodsName',title:'商品名称',width:100},
		    {field:'num',title:'库存数量',width:100},
		]],
		singleSelect:true,
		pagination:true
	});
})
	
	