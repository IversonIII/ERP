public class CreateOrderXls {

	/**
	 * 创建订单表格模板
	 * @param args
	 */
	public static void main(String[] args) {
		//工作簿
		HSSFWorkbook hssfWorkbook = new HSSFWorkbook();
		//表
		HSSFSheet hssfSheet = hssfWorkbook.createSheet("采购订单")
		
	}

}
